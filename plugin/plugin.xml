<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

     <extension-point id="interpreters" name="interpreters" schema="schema/interpreters.exsd"/>
   <extension
         point="org.eclipse.core.runtime.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.text"
            id="Scheme"
            name="Scheme File"
            file-extensions="%Extensions"
            priority="normal"/>
      <file-association
            content-type="org.schemeway.plugins.schemescript.Scheme"
            file-extensions="%Extensions"/>
      <content-type
            base-type="org.eclipse.core.runtime.text"
            id="BRL"
            name="BRL File"
            file-extensions="%BrlExtensions"
            priority="normal"/>
      <file-association
            content-type="org.schemeway.plugins.schemescript.BRL"
            file-extensions="%BrlExtensions"/>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="Scheme Editor"
            default="true"
            icon="icons/scheme.gif"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            class="org.schemeway.plugins.schemescript.editor.SchemeEditor"
            extensions="%Extensions,%BrlExtensions"
            id="org.schemeway.plugins.schemescript.editor.SchemeEditor">
         <contentTypeBinding contentTypeId="org.schemeway.plugins.schemescript.Scheme"/>
         <contentTypeBinding contentTypeId="org.schemeway.plugins.schemescript.BRL"/>
      </editor>
   </extension>
   
   <extension
         point="org.eclipse.core.filebuffers.documentSetup">
      <participant
            class="org.schemeway.plugins.schemescript.editor.SchemeDocumentSetupParticipant"
            contentTypeId="org.schemeway.plugins.schemescript.Scheme"
            extensions="%Extensions"/>
      <participant
            class="org.schemeway.plugins.schemescript.editor.BrlDocumentSetupParticipant"
            contentTypeId="org.schemeway.plugins.schemescript.BRL"
            extensions="%BrlExtensions"/>
   </extension>
   <extension
	   	 point="org.eclipse.core.filebuffers.annotationModelCreation">
      <factory
            class="org.eclipse.ui.texteditor.ResourceMarkerAnnotationModelFactory"
            contentTypeId="org.schemeway.plugins.schemescript.Scheme"
            extensions="%Extensions">
	  </factory>
      <factory
            class="org.eclipse.ui.texteditor.ResourceMarkerAnnotationModelFactory"
            contentTypeId="org.schemeway.plugins.schemescript.BRL"
            extensions="%BrlExtensions">
	  </factory>
   </extension>

   <extension
         point="org.eclipse.ui.contexts">
      <context
            name="Editing Scheme"
            description="Scheme Editor Context"
            id="org.schemeway.plugins.schemescript.schemeEditorScope"
            parentId="org.eclipse.ui.textEditorScope">
      </context>
   </extension>
   <extension
         id="error"
         name="Scheme Error"
         point="org.eclipse.core.resources.markers">
      <super type="org.eclipse.core.resources.problemmarker"/>
      <super type="org.eclipse.core.resources.textmarker"/>
   </extension>
   
